# Example secrets file for local development (DO NOT commit real secrets)
# Copy this to `server/.env` locally or set each value in your environment/CI secrets.

# Google Gemini (Generative AI)
GOOGLE_GEMINI_API_KEY=your_google_gemini_api_key_here

# Pinecone (Vector DB)
PINECONE_API_KEY=your_pinecone_api_key_here
PINECONE_ENVIRONMENT=your_pinecone_environment_here
PINECONE_INDEX_NAME=prochat-ai-index

# S3 / MinIO (for local dev you can use MinIO)
S3_ENDPOINT=http://localhost:9000
S3_REGION=us-east-1
AWS_ACCESS_KEY_ID=minioadmin
AWS_SECRET_ACCESS_KEY=minioadmin123
S3_BUCKET_NAME=prochat-uploads

# Redis (optional)
REDIS_HOST=localhost
REDIS_PORT=6379

# JWT Secret
JWT_SECRET=replace_with_strong_jwt_secret

# Optional mock flags for local testing
MOCK_AI=true
MOCK_VECTOR=true
